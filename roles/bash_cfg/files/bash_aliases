alias cp='cp -v'
alias debver='cat /etc/debian_version'
alias funcs='typeset -F'
alias h='history'
alias la='ls -A'
alias mkdir='mkdir -pv'
alias mv='mv -v'
alias path='echo -e ${PATH//:/\\n}'
alias pypath='echo -e ${PYTHONPATH//:/\\n}'
alias ps='ps aux'
alias pubip='echo $(curl -s ifconfig.me)'
alias privip='ifconfig'
alias rm='rm -v'
alias runftp='sudo -s launchctl load -w /System/Library/LaunchDaemons/ftp.plist'
alias killftp='sudo -s launchctl unload -w /System/Library/LaunchDaemons/ftp.plist'
